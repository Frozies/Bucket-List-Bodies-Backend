{
  "name": "bucket-list-bodies-backend",
  "version": "1.0.0",
  "description": "Backend for bucket-list-bodies",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node src/index.ts\"",
    "start": "npm run build && ts-node src/index.js",
    "build": "npm install --only=dev && tsc",
    "test": "mocha -r ts-node/register"
  },
  "author": "Davin Young",
  "dependencies": {
    "apollo-datasource-mongodb": "^0.4.0",
    "apollo-server-express": "^3.1.2",
    "aws-sdk": "^2.959.0",
    "cloudinary": "^1.26.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "graphql-upload": "^12.0.0",
    "lodash": "^4.17.21",
    "mongodb-memory-server": "^7.3.6",
    "mongoose": "^5.12.8",
    "promises-all": "^1.0.0",
    "stripe": "^8.167.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/express": "^4.17.13",
    "@types/graphql-upload": "^8.0.6",
    "@types/lodash": "^4.14.172",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.4.13",
    "chai": "^4.3.4",
    "graphql-tools": "^7.0.5",
    "mocha": "^9.1.1",
    "nodemon": "^2.0.12",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  },
  "mocha": {
    "file": ["./tests/mockDB.ts"],
    "spec": ["tests/**/*.test.ts"],
    "watch-files": [
      "tests/**/*.ts"
    ],
    "timeout": 60000
  }
}
